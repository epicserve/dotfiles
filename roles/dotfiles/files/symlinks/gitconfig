[color]
    diff = auto
    status = auto
    branch = auto
[user]
	name = Brent O'Connor
	email = epicserve@gmail.com
[oconnor]
    name = Brent O'Connor
    email = epicserve@gmail.com
[core]
    editor = code --wait
    excludesfile = ~/.gitignore
[alias]
    st = status
    d = diff
    ci = commit -v
    cia = commit -v -a
    co = checkout
    cp = cherry-pick
    dci = svn dcommit
    l = log
    ll = log -p
    lt = log trunk..
    llt = log -p trunk..
    lm = log master..
    llm = log -p master..
    b = branch
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    up = "!sh -c 'git checkout --quiet --detach HEAD && \
                    git fetch --quiet origin master:master develop:develop demo:demo test:test; \
                    git checkout --quiet -'"
[git-tmbundle]
    gitnub-path = /usr/local/bin/git
[push]
	default = simple
[merge]
	ff = only
[pager]
	branch = false
